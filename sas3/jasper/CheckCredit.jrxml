	<subDataset name="SO" uuid="6bcda85a-2e1d-4d0f-a7de-b01e5ab06229">
		<parameter name="C_BPartner_ID" class="java.math.BigDecimal">
			<defaultValueExpression><![CDATA[]]></defaultValueExpression>
		</parameter>
		<queryString>
			<![CDATA[SELECT
	o.DateOrdered,
	o.DocumentNo,
	o.Description,
	o.GrandTotal,
	COALESCE(SUM(il.LineNetAmt),0) AS Invoiced
FROM C_Order o
INNER JOIN C_DocType dt ON (dt.C_DocType_ID = o.C_DocType_ID)
INNER JOIN C_OrderLine ol ON (ol.C_Order_ID = o.C_Order_ID)
LEFT JOIN C_InvoiceLine il ON (il.C_OrderLine_ID = ol.C_OrderLine_ID)
LEFT JOIN C_Invoice i ON (i.C_Invoice_ID = il.C_Invoice_ID)
WHERE o.Bill_BPartner_ID=$P{C_BPartner_ID}
AND o.DocStatus IN ('CO','CL')
AND (i.DocStatus IS NULL OR i.DocStatus IN ('CO','CL'))
AND o.IsSOTrx='Y'
AND dt.DocBaseType = 'SOO'
AND dt.DocSubTypeSO = 'SO'
AND ol.QtyOrdered > ol.QtyInvoiced
GROUP BY o.DateOrdered,o.DocumentNo,o.Description,o.Grandtotal
ORDER BY o.DateOrdered]]>
		</queryString>
		<field name="dateordered" class="java.sql.Timestamp"/>
		<field name="documentno" class="java.lang.String"/>
		<field name="description" class="java.lang.String"/>
		<field name="grandtotal" class="java.math.BigDecimal"/>
		<field name="invoiced" class="java.math.BigDecimal"/>
		<variable name="sisa" class="java.math.BigDecimal">
			<variableExpression><![CDATA[$F{grandtotal}.subtract( $F{invoiced} )]]></variableExpression>
		</variable>
		<variable name="accumulated" class="java.math.BigDecimal" calculation="Sum">
			<variableExpression><![CDATA[$V{sisa}]]></variableExpression>
			<initialValueExpression><![CDATA[0]]></initialValueExpression>
		</variable>
	</subDataset>
	<subDataset name="Invoice" uuid="2a09c86f-6d57-4846-a41f-feb9a14cecd5">
		<parameter name="C_BPartner_ID" class="java.math.BigDecimal">
			<defaultValueExpression><![CDATA[]]></defaultValueExpression>
		</parameter>
		<queryString>
			<![CDATA[SELECT * FROM
(
SELECT
	i.DateInvoiced,
	i.DocumentNo,
	i.Description,
	i.DueDate,
	paymenttermduedays(i.c_paymentterm_id,i.dateinvoiced,getdate()) AS NetDays,
	i.GrandTotal,
	invoicepaid(i.c_invoice_id, i.c_currency_id, (1)::numeric) AS paid
FROM c_invoice_v i
LEFT JOIN C_DocType dt ON (dt.C_DocType_ID = i.C_DocType_ID)
LEFT JOIN(
	SELECT SUM(alx.Amount+alx.DiscountAmt+alx.WriteOffAmt) Amount,alix.C_Invoice_ID
	FROM C_AllocationLine alx
	INNER JOIN C_AllocationHdr alhx ON (alhx.C_AllocationHdr_ID = alx.C_AllocationHdr_ID)
	INNER JOIN C_Payment px ON (px.C_Payment_ID = alx.C_Payment_ID)
	INNER JOIN C_AllocationLine alix ON (alix.C_Invoice_ID>0 AND alix.C_AllocationHdr_ID = alhx.C_AllocationHdr_ID)
	WHERE alhx.DocStatus IN ('CO','CL')
	AND px.DocStatus IN ('CO','CL')
	AND px.IsReceipt = 'Y'
	AND px.TenderType = 'K'
	--AND px.EftValutaDate > CURRENT_DATE
	AND px.IsCashed='N'
	GROUP BY alix.C_Invoice_ID
) g ON (i.C_Invoice_ID = g.C_Invoice_ID)
WHERE i.C_BPartner_ID=$P{C_BPartner_ID}
AND i.IsSOTrx = 'Y'
AND i.DocStatus IN ('CO','CL')
AND NOT (dt.DocBaseType = 'ARC' OR i.C_DocType_ID IN (550294,550293))
AND g.Amount IS NULL

UNION

SELECT
	i.DateInvoiced,
	i.DocumentNo,
	i.Description,
	i.DueDate,
	paymenttermduedays(i.c_paymentterm_id,i.dateinvoiced,getdate()) AS NetDays,
	i.GrandTotal,
	invoicepaid(i.c_invoice_id, i.c_currency_id, (1)::numeric) - g.Amount AS paid
FROM C_Invoice_V i
LEFT JOIN C_DocType dt ON (dt.C_DocType_ID = i.C_DocType_ID)
LEFT JOIN(
	SELECT SUM(alx.Amount+alx.DiscountAmt+alx.WriteOffAmt) Amount,alx.C_Invoice_ID
	FROM C_AllocationLine alx
	INNER JOIN C_AllocationHdr alhx ON (alhx.C_AllocationHdr_ID = alx.C_AllocationHdr_ID)
	INNER JOIN C_Payment px ON (px.C_Payment_ID = alx.C_Payment_ID)
	WHERE alhx.DocStatus IN ('CO','CL')
	AND px.DocStatus IN ('CO','CL')
	AND px.IsReceipt = 'Y'
	AND px.TenderType = 'K'
	--AND px.EftValutaDate > CURRENT_DATE
	AND px.IsCashed='N'
	GROUP BY alx.C_Invoice_ID
) g ON (i.C_Invoice_ID = g.C_Invoice_ID)
WHERE i.C_BPartner_ID=$P{C_BPartner_ID}
AND i.IsSOTrx = 'Y'
AND i.DocStatus IN ('CO','CL')
AND NOT (dt.DocBaseType = 'ARC' OR i.C_DocType_ID IN (550294,550293))
AND g.Amount > 0
) z
WHERE z.GrandTotal <> z.Paid
ORDER BY z.DateInvoiced]]>
		</queryString>
