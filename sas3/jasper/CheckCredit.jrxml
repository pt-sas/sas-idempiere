	<subDataset name="BPCredit" uuid="0f66f8cf-0ad0-4137-975b-00fb02b30614">
		<parameter name="C_BPartner_ID" class="java.math.BigDecimal">
			<defaultValueExpression><![CDATA[]]></defaultValueExpression>
		</parameter>
		<queryString>
			<![CDATA[SELECT
	CASE WHEN orgt.AD_Org_ID=0 THEN '-' ELSE orgt.Name END ORGTRX,
	pt.Name PAYMENTTERM,
	sr.Name SALESREP,
	br.SO_CreditLimit,
	(SELECT COALESCE(SUM((olx.QtyOrdered-olx.QtyInvoiced) * olx.PriceActual),0)
	FROM C_OrderLine olx
	INNER JOIN C_Order ox ON (ox.C_Order_ID = olx.C_Order_ID)
	INNER JOIN C_DocType dtx ON (dtx.C_DocType_ID = ox.C_DocType_ID)
	WHERE ox.DocStatus IN ('CO','CL')
	AND dtx.DocBaseType = 'SOO'
	AND dtx.DocSubTypeSO = 'SO'
	AND ox.Bill_BPartner_ID = bp.C_BPartner_ID
	AND COALESCE(ox.AD_OrgTrx_ID,0) = orgt.AD_Org_ID
	) SO_BELUM_INVOICE,
	(SELECT COALESCE(SUM(ix.GrandTotal),0)
	FROM C_Invoice ix
	INNER JOIN C_DocType dtx ON (dtx.C_DocType_ID = ix.C_DocType_ID)
	WHERE ix.DocStatus IN ('CO','CL')
	AND ix.IsSOTrx='Y'
	AND NOT (dtx.DocBaseType = 'ARC' OR dtx.C_DocType_ID IN (550294,550293))
	AND ix.C_BPartner_ID = bp.C_BPartner_ID
	AND COALESCE(ix.AD_OrgTrx_ID,0) = orgt.AD_Org_ID
	) INVOICE_TOTAL,
	(SELECT COALESCE(SUM(alx.Amount+alx.DiscountAmt+alx.WriteOffAmt),0)
	FROM C_AllocationLine alx
	INNER JOIN C_AllocationHdr alhx ON (alhx.C_AllocationHdr_ID = alx.C_AllocationHdr_ID)
	INNER JOIN C_Invoice ix ON (ix.C_Invoice_ID = alx.C_Invoice_ID)
	INNER JOIN C_DocType dtx ON (dtx.C_DocType_ID = ix.C_DocType_ID)
	WHERE alhx.DocStatus IN ('CO','CL')
	AND ix.IsSOTrx='Y'
	AND NOT (dtx.DocBaseType = 'ARC' OR dtx.C_DocType_ID IN (550294,550293))
	AND ix.DocStatus IN ('CO','CL')
	AND ix.C_BPartner_ID = bp.C_BPartner_ID
	AND COALESCE(ix.AD_OrgTrx_ID,0) = orgt.AD_Org_ID
	) ALOKASI_TOTAL,
	(SELECT COALESCE(SUM(alx.Amount+alx.DiscountAmt+alx.WriteOffAmt),0)
	FROM C_AllocationLine alx
	INNER JOIN C_AllocationHdr alhx ON (alhx.C_AllocationHdr_ID = alx.C_AllocationHdr_ID)
	INNER JOIN C_Payment px ON (px.C_Payment_ID = alx.C_Payment_ID)
	WHERE alhx.DocStatus IN ('CO','CL')
	AND px.DocStatus IN ('CO','CL')
	AND px.IsReceipt = 'Y'
	AND px.C_BPartner_ID = bp.C_BPartner_ID
	AND COALESCE(px.AD_OrgTrx_ID,0) = orgt.AD_Org_ID
	AND px.TenderType = 'K'
	--AND px.EftValutaDate > CURRENT_DATE
	AND px.IsCashed = 'N'
	) ALOKASI_GIRO_BELUM_CAIR,
	(SELECT AVG(EXTRACT(DAY FROM alhx.DateAcct - ix.DateInvoiced))
	FROM C_Invoice ix
	INNER JOIN C_DocType dtx ON (dtx.C_DocType_ID = ix.C_DocType_ID)
	INNER JOIN C_AllocationLine alx ON (alx.C_Invoice_ID = ix.C_Invoice_ID)
	INNER JOIN C_AllocationHdr alhx ON (alhx.C_AllocationHdr_ID = alx.C_AllocationHdr_ID)
	--INNER JOIN C_allocationLine alpx ON (alpx.C_AllocationHdr_ID = alhx.C_AllocationHdr_ID)
	--INNER JOIN C_Payment px ON (px.C_Payment_ID = alpx.C_Payment_ID)
	WHERE alhx.DocStatus IN ('CO','CL')
	AND ix.IsSOTrx='Y'
	AND NOT (dtx.DocBaseType = 'ARC' OR dtx.C_DocType_ID IN (550294,550293))
	AND ix.DocStatus IN ('CO','CL')
	--AND px.DocStatus IN ('CO','CL')
	AND ix.IsPaid = 'Y'
	AND ix.C_BPartner_ID = bp.C_BPartner_ID
	AND COALESCE(ix.AD_OrgTrx_ID,0) = orgt.AD_Org_ID
	) AVG_PAY,
	(SELECT AVG(EXTRACT(DAY FROM alhx.DateAcct - ix.DateInvoiced))
	FROM C_Invoice ix
	INNER JOIN C_DocType dtx ON (dtx.C_DocType_ID = ix.C_DocType_ID)
	INNER JOIN C_AllocationLine alx ON (alx.C_Invoice_ID = ix.C_Invoice_ID)
	INNER JOIN C_AllocationHdr alhx ON (alhx.C_AllocationHdr_ID = alx.C_AllocationHdr_ID)
	--INNER JOIN C_allocationLine alpx ON (alpx.C_AllocationHdr_ID = alhx.C_AllocationHdr_ID)
	--INNER JOIN C_Payment px ON (px.C_Payment_ID = alpx.C_Payment_ID)
	WHERE alhx.DocStatus IN ('CO','CL')
	AND ix.IsSOTrx='Y'
	AND NOT (dtx.DocBaseType = 'ARC' OR dtx.C_DocType_ID IN (550294,550293))
	AND ix.DocStatus IN ('CO','CL')
	--AND px.DocStatus IN ('CO','CL')
	AND ix.IsPaid = 'Y'
	AND ix.C_BPartner_ID = bp.C_BPartner_ID
	AND COALESCE(ix.AD_OrgTrx_ID,0) = orgt.AD_Org_ID
	AND ix.DateInvoiced >= ADDDAYS(CURRENT_DATE,-90)
	) AVG_PAY_90,
	(SELECT AVG(EXTRACT(DAY FROM alhx.DateAcct - ix.DateInvoiced))
	FROM C_Invoice ix
	INNER JOIN C_DocType dtx ON (dtx.C_DocType_ID = ix.C_DocType_ID)
	INNER JOIN C_AllocationLine alx ON (alx.C_Invoice_ID = ix.C_Invoice_ID)
	INNER JOIN C_AllocationHdr alhx ON (alhx.C_AllocationHdr_ID = alx.C_AllocationHdr_ID)
	--INNER JOIN C_allocationLine alpx ON (alpx.C_AllocationHdr_ID = alhx.C_AllocationHdr_ID)
	--INNER JOIN C_Payment px ON (px.C_Payment_ID = alpx.C_Payment_ID)
	WHERE alhx.DocStatus IN ('CO','CL')
	AND ix.IsSOTrx='Y'
	AND NOT (dtx.DocBaseType = 'ARC' OR dtx.C_DocType_ID IN (550294,550293))
	AND ix.DocStatus IN ('CO','CL')
	--AND px.DocStatus IN ('CO','CL')
	AND ix.IsPaid = 'Y'
	AND ix.C_BPartner_ID = bp.C_BPartner_ID
	AND COALESCE(ix.AD_OrgTrx_ID,0) = orgt.AD_Org_ID
	AND ix.DateInvoiced >= ADDDAYS(CURRENT_DATE,-180)
	) AVG_PAY_180,
	(SELECT COALESCE(SUM(ix.GrandTotal),0)
	FROM RV_C_Invoice ix
	INNER JOIN C_Invoice ixx ON (ixx.C_Invoice_ID = ix.C_Invoice_ID)
	INNER JOIN C_DocType dtx ON (dtx.C_DocType_ID = ix.C_DocType_ID)
	WHERE ix.DocStatus IN ('CO','CL')
	AND (dtx.DocBaseType = 'ARC' OR dtx.C_DocType_ID IN (550294,550293))
	AND ix.C_BPartner_ID = bp.C_BPartner_ID
	AND COALESCE(ixx.AD_OrgTrx_ID,0) = orgt.AD_Org_ID
	) CREDIT_NOTE,
	(SELECT COALESCE(SUM(px.PayAmt),0)
	FROM C_Payment px
	WHERE px.IsReceipt = 'Y'
	AND px.DocStatus IN ('CO','CL')
	AND px.C_BPartner_ID = bp.C_BPartner_ID
	AND COALESCE(px.AD_OrgTrx_ID,0) = orgt.AD_Org_ID
	) TOTAL_PAYMENT,
	(SELECT COALESCE(SUM(alx.Amount+alx.DiscountAmt+alx.WriteOffAmt),0)
	FROM C_Payment px
	INNER JOIN C_AllocationLine alx ON (alx.C_Payment_ID = px.C_Payment_ID)
	INNER JOIN C_AllocationHdr alhx ON (alhx.C_AllocationHdr_ID = alx.C_AllocationHdr_ID)
	WHERE px.IsReceipt = 'Y'
	AND px.DocStatus IN ('CO','CL')
	AND alhx.DocStatus IN ('CO','CL')
	AND px.C_BPartner_ID = bp.C_BPartner_ID
	AND COALESCE(px.AD_OrgTrx_ID,0) = orgt.AD_Org_ID
	) TOTAL_PAYMENT_ALLOCATED,
	--(SELECT COALESCE(SUM(px.PayAmt - px.AllocatedAmt),0)
	--FROM RV_Payment px
	--WHERE px.IsReceipt = 'Y'
	--AND px.TenderType = 'K'
	--AND px.IsAllocated = 'N'
	--AND px.DocStatus IN ('CO','CL')
	--AND px.C_BPartner_ID = bp.C_BPartner_ID
	--AND COALESCE(px.AD_OrgTrx_ID,0) = orgt.AD_Org_ID
	--) GIRO_BELUM_ALOKASI,
	(SELECT COALESCE(SUM(px.PayAmt),0)
	FROM C_Payment px
	WHERE px.IsReceipt = 'Y'
	AND px.TenderType = 'K'
	AND px.DocStatus IN ('CO','CL')
	AND px.C_BPartner_ID = bp.C_BPartner_ID
	AND COALESCE(px.AD_OrgTrx_ID,0) = orgt.AD_Org_ID
	) TOTAL_GIRO,
	(SELECT COALESCE(SUM(alx.Amount+alx.DiscountAmt+alx.WriteOffAmt),0)
	FROM C_Payment px
	INNER JOIN C_AllocationLine alx ON (alx.C_Payment_ID = px.C_Payment_ID)
	INNER JOIN C_AllocationHdr alhx ON (alhx.C_AllocationHdr_ID = alx.C_AllocationHdr_ID)
	WHERE px.IsReceipt = 'Y'
	AND px.TenderType = 'K'
	AND px.DocStatus IN ('CO','CL')
	AND alhx.DocStatus IN ('CO','CL')
	AND px.C_BPartner_ID = bp.C_BPartner_ID
	AND COALESCE(px.AD_OrgTrx_ID,0) = orgt.AD_Org_ID
	) TOTAL_GIRO_ALLOCATED
FROM C_BPartner bp
LEFT JOIN AD_Org orgt ON (orgt.IsOrgTrxDim='Y' OR orgt.AD_Org_ID = 0)
LEFT JOIN SAS_BPRule br ON (br.C_BPartner_ID = bp.C_BPartner_ID AND br.AD_OrgTrx_ID = orgt.AD_Org_ID)
LEFT JOIN C_PaymentTerm pt ON (pt.C_PaymentTerm_ID = br.C_PaymentTerm_ID)
LEFT JOIN AD_User sr ON (sr.AD_User_ID = br.SalesRep_ID)
WHERE bp.C_BPartner_ID = $P{C_BPartner_ID}
ORDER BY orgt.Name]]>
		</queryString>
		<field name="orgtrx" class="java.lang.String"/>
		<field name="paymentterm" class="java.lang.String"/>
		<field name="salesrep" class="java.lang.String"/>
		<field name="so_creditlimit" class="java.math.BigDecimal"/>
