	</parameter>
	<queryString>
		<![CDATA[SELECT
	bp.C_BPartner_ID,
	bp.Value AS BP_Value,
	bp.Name AS BP_Name
FROM C_BPartner bp
WHERE bp.C_BPartner_ID=COALESCE($P{RECORD_ID},$P{C_BPartner_ID})]]>
