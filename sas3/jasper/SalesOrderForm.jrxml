	<subDataset name="CreditBP" uuid="af3dc438-5502-435a-85e0-e1e83cf39a89">
		<parameter name="C_Order_ID" class="java.lang.Integer">
			<defaultValueExpression><![CDATA[]]></defaultValueExpression>
		</parameter>
		<queryString>
			<![CDATA[SELECT
	(SELECT MAX(bpr.SO_CreditLimit)
	 FROM SAS_BPRule bpr
	 WHERE bpr.C_BPartner_ID=o.Bill_BPartner_ID
	 AND bpr.AD_OrgTrx_ID=o.AD_OrgTrx_ID) AS SO_CreditLimit,
	(	(SELECT COALESCE(SUM(ox.GrandTotal),0)
		FROM C_Order ox
		INNER JOIN C_DocType dt ON (dt.C_DocType_ID = ox.C_DocType_ID)
		WHERE ox.IsSOTrx = 'Y' AND ox.DocStatus IN ('CO','IP')
		AND dt.DocBaseType = 'SOO' AND dt.DocSubTypeSO = 'SO'
		AND ox.C_Order_ID <> o.C_Order_ID AND ox.Bill_BPartner_ID = o.Bill_BPartner_ID
		AND ox.AD_OrgTrx_ID = o.AD_OrgTrx_ID)
	 + o.GrandTotal
	 + (SELECT COALESCE(SUM((ol.QtyOrdered * ol.PriceActual) +
		CASE WHEN pl.IsTaxIncluded = 'N' THEN (ol.QtyOrdered * ol.PriceActual * t.Rate / 100) ELSE 0 END),0)
		FROM C_Order ox
		INNER JOIN C_OrderLine ol ON (ol.C_Order_ID = ox.C_Order_ID)
		INNER JOIN C_Tax t ON (t.C_Tax_ID = ol.C_Tax_ID)
		INNER JOIN C_DocType dt ON (dt.C_DocType_ID = ox.C_DocType_ID)
		INNER JOIN M_PriceList pl ON (pl.M_PriceList_ID = ox.M_PriceList_ID)
		WHERE ox.IsSOTrx = 'Y' AND ox.DocStatus IN ('CL')
		AND dt.DocBaseType = 'SOO' AND dt.DocSubTypeSO = 'SO'
		AND ox.C_Order_ID <> o.C_Order_ID AND ox.Bill_BPartner_ID = o.Bill_BPartner_ID
		AND ox.AD_OrgTrx_ID = o.AD_OrgTrx_ID)
	 - (SELECT COALESCE(SUM(al.Amount),0)
	 FROM C_AllocationLine al
	 INNER JOIN C_AllocationHdr ah ON (ah.C_AllocationHdr_ID = al.C_AllocationHdr_ID)
	 INNER JOIN C_Invoice i ON (i.C_Invoice_ID = al.C_Invoice_ID)
	 LEFT JOIN C_Payment py ON (py.C_Payment_ID = al.C_Payment_ID)
	 WHERE i.IsSOTrx='Y' AND ah.DocStatus IN ('CO','CL')
	 AND al.C_BPartner_ID = o.Bill_BPartner_ID AND i.AD_OrgTrx_ID = o.AD_OrgTrx_ID)
	 + (SELECT COALESCE(SUM(al.Amount),0)
	 FROM C_AllocationLine al
	 INNER JOIN C_AllocationHdr ah ON (ah.C_AllocationHdr_ID = al.C_AllocationHdr_ID)
	 INNER JOIN C_Invoice i ON (i.C_Invoice_ID = al.C_Invoice_ID)
	 LEFT JOIN C_Payment py ON (py.C_Payment_ID = al.C_Payment_ID)
	 WHERE i.IsSOTrx='Y' AND ah.DocStatus IN ('CO','CL')
	 AND al.C_BPartner_ID = o.Bill_BPartner_ID AND i.AD_OrgTrx_ID = o.AD_OrgTrx_ID
	 AND ADDDAYS(ah.DateTrx,2) > CURRENT_DATE
	 AND py.TenderType = 'K')) AS SO_CreditUsed
FROM C_Order o
WHERE o.C_Order_ID=$P{C_Order_ID}]]>
		</queryString>
		<field name="so_creditlimit" class="java.math.BigDecimal"/>
		<field name="so_creditused" class="java.math.BigDecimal"/>
	</subDataset>
	<subDataset name="Aging" uuid="d3757a8d-523d-41c1-ac85-c10d2ca286aa">
		<parameter name="C_Order_ID" class="java.lang.Integer">
			<defaultValueExpression><![CDATA[]]></defaultValueExpression>
		</parameter>
		<queryString>
			<![CDATA[SELECT
	 current_date-(i.DateInvoiced+pt.NetDays+pt.GraceDays) AS AgingOver
FROM C_Order o
INNER JOIN C_Invoice i ON (i.C_BPartner_ID=o.C_BPartner_ID AND i.DocStatus IN ('CO','CL') AND i.IsPaid='N')
INNER JOIN C_PaymentTerm pt ON (pt.C_PaymentTerm_ID=i.C_PaymentTerm_ID)
WHERE o.C_Order_ID=$P{C_Order_ID}
ORDER BY i.DateInvoiced LIMIT 1]]>
		</queryString>
		<field name="agingover" class="java.lang.Integer"/>
	</subDataset>
	<subDataset name="PriceLimit" uuid="4f0d855f-f2df-4cf3-b2bd-87f57d546f90">
		<parameter name="C_Order_ID" class="java.lang.Integer">
			<defaultValueExpression><![CDATA[]]></defaultValueExpression>
		</parameter>
		<queryString>
			<![CDATA[SELECT MIN(ol.Std_Discount - ol.Discount) AS PriceLimit
FROM C_OrderLine ol
WHERE ol.C_Order_ID = $P{C_Order_ID}
AND ol.M_Product_ID > 0]]>
		</queryString>
		<field name="pricelimit" class="java.math.BigDecimal"/>
	</subDataset>
	<subDataset name="Discount" uuid="6ed3d5ae-17ef-4d4d-a5a8-cc39de8c75d4">
		<parameter name="C_Order_ID" class="java.lang.Integer">
			<defaultValueExpression><![CDATA[]]></defaultValueExpression>
		</parameter>
		<queryString>
			<![CDATA[SELECT
	(SELECT SUM(olx.PriceList*olx.QtyOrdered) FROM C_OrderLine olx WHERE olx.C_Order_ID=o.C_Order_ID) AS TotalPrice,
	o.TotalLines
FROM C_Order o
WHERE o.C_Order_ID=$P{C_Order_ID}]]>
		</queryString>
		<field name="totalprice" class="java.math.BigDecimal"/>
		<field name="totallines" class="java.math.BigDecimal"/>
		<variable name="DISCTOTAL" class="java.math.BigDecimal">
			<variableExpression><![CDATA[$F{totalprice} > 0 ?
(($F{totalprice}.subtract( $F{totallines} )).multiply(new BigDecimal(100)).divide($F{totalprice}, 2, RoundingMode.HALF_UP))
:
0]]></variableExpression>
		</variable>
	</subDataset>
	<parameter name="RECORD_ID" class="java.lang.Integer"/>
	<parameter name="C_Order_ID" class="java.lang.Integer"/>
	<queryString>
		<![CDATA[SELECT
	o.DateOrdered,
	o.DocumentNo,
	bpl.Name BP_NAME,
	regexp_replace(parse('C_BPartner_Location',o.Bill_Location_ID,0), E'[\\n\\r]+', ' ', 'g' ) BP_LOCATION,
	o.POReference,
	COALESCE(p.Name,ch.Name) PRODUCT_NAME,
	p.Value PRODUCT_CODE,
	ol.QtyOrdered,
	parse('C_UOM',ol.C_UOM_ID,1) UOM_NAME,
	ol.PriceEntered,
	t.Name TAX_NAME,
	parse('AD_Org',o.AD_Org_ID,2) ORG_NAME,
	ol.DatePromised,
	parse('C_Location',oi.C_Location_ID,0) ORG_LOCATION,
	oi.Phone,
	oi.Phone2,
	oi.Fax,
	parse('AD_User',o.AD_User_ID,1) USER_NAME,
	parse('C_Currency',o.C_Currency_ID,0) ISO_CODE,
	parse('AD_User',o.SalesRep_ID,2) SALES_NAME,
	parse('C_PaymentTerm',o.C_PaymentTerm_ID,0) PAYMENTTERM_VALUE,
	bp.Value BP_VALUE,
	(SELECT p.Name FROM M_Pricelist p WHERE p.M_Pricelist_ID = o.M_Pricelist_ID) PRICELIST,
	o.TotalLines,
	o.GrandTotal,
	o.Description,
	parse('AD_User',o.DropShip_User_ID,1) DROPSHIP_USER_NAME,
	parse('C_BPartner',o.DropShip_BPartner_ID,1) DROPSHIP_BP_NAME,
	regexp_replace(parse('C_BPartner_Location',o.DropShip_Location_ID,0), E'[\\n\\r]+', ' ', 'g' ) DROPSHIP_LOCATION,
	o.DocStatus,
	parse('AD_User',o.updatedBy,1) UPDATED_BY,
	ol.LineNetAmt,
	ol.QtyEntered,
	parse('C_Charge',ol.C_Charge_ID,1) CHARGE,
	COALESCE((SELECT SUM(ot.TaxAmt) FROM C_OrderTax ot WHERE ot.C_Order_ID = ol.C_Order_ID),0) TaxAmt,
	o.IsTaxIncluded,
	getreflist('ALL','DELIVERYVIARULE',o.DeliveryRule) AS DeliveryVia,
	(SELECT rlx.Name FROM AD_Ref_List rlx WHERE rlx.Value=o.InvoiceRule AND rlx.AD_Reference_ID=150) AS InvoiceRule,
	o.PickListNote,
	(SELECT rlx.Name FROM AD_Ref_List rlx WHERE rlx.Value=o.PriorityRule AND rlx.AD_Reference_ID=154) AS Priority,
	ol.std_discount,
	o.z_wfscenariocredit_id,
	o.z_wfscenarioaging_id,
	o.z_wfscenarioprice_id,
	o.z_wfscenariograndtotal_id,
	o.c_doctypetarget_id,
	p.m_product_category_id,
	ol.pricelist as LINE_PRICELIST,
	ol.priceactual,
	ol.c_charge_id,
	c.Value AS CAMPAIGNVALUE,
	dl.Name AS DISCOUNT1,
	round(ol.line/10,0) linegroup,
	ol.discount,
	ol.line
FROM C_OrderLine ol
INNER JOIN C_Order o ON (o.C_Order_ID = ol.C_Order_ID)
LEFT JOIN C_BPartner bp ON (bp.C_BPartner_ID = o.Bill_BPartner_ID)
LEFT JOIN C_BPartner_Location bpl ON (bpl.C_BPartner_Location_ID = o.Bill_Location_ID)
LEFT JOIN C_Tax t ON (t.C_Tax_ID = ol.C_Tax_ID)
INNER JOIN AD_OrgInfo oi ON (oi.AD_Org_ID = o.AD_Org_ID)
LEFT JOIN M_PriceList pl ON (pl.M_PriceList_ID = o.M_PriceList_ID)
LEFT JOIN M_Product p ON (p.M_Product_ID=ol.M_Product_ID)
LEFT JOIN C_Charge ch ON (ch.C_Charge_ID = ol.C_Charge_ID)
LEFT JOIN C_Campaign c ON (c.C_Campaign_ID=ol.C_Campaign_ID)
LEFT JOIN SAS_DiscountList dl ON (dl.SAS_DiscountList_ID=ol.SAS_DiscountList_ID)
WHERE	1 = 1
	AND o.C_Order_ID = COALESCE($P{RECORD_ID},$P{C_Order_ID})
	AND ol.M_Product_ID > 0
ORDER BY ol.Line]]>
	</queryString>
