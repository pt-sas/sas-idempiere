	<queryString>
		<![CDATA[SELECT z.*,x.*,
(SELECT COALESCE(SUM(ilx.LineNetAmt),0) FROM C_InvoiceLine ilx
INNER JOIN C_Invoice ix ON (ix.C_Invoice_ID = ilx.C_Invoice_ID)
WHERE ix.Z_TaxInvoice_ID = COALESCE($P{RECORD_ID},$P{Z_TaxInvoice_ID})
AND ilx.C_Charge_ID = 2000074) AS DOC_DISCOUNT
FROM
(
SELECT
	ti.Z_TaxInvoice_ID,
	ti.DocumentNo TAXINVOICE_NO,
	ti.DateInvoiced,
	i.POReference,
	parse('C_PaymentTerm',i.C_PaymentTerm_ID,1) PAYMENT_TERM,
	bp.Name BP_NAME,
	bp.Value BP_CODE,
	parse('C_BPartner_Location',COALESCE(bpl.C_BPartner_Location_ID,i.C_BPartner_Location_ID),0) BP_LOCATION,
	bp.TaxID BP_TAXID,
	parse('C_Currency',i.C_Currency_ID,0) CURRENCY,
	parse('AD_Org',i.AD_Org_ID,0) ORG_DESC,
	parse('C_Location',oi.C_Location_ID,0) ORG_LOCATION,
	oi.Phone ORG_PHONE,
	oi.Fax ORG_FAX,
	oi.TaxID ORG_TAXID,
	COALESCE(p.Name,c.Name) PRODUCT_NAME,
	COALESCE(p.Value,c.Name) PRODUCT_CODE,
	COALESCE(p.Description,c.Description) PRODUCT_DESCRIPTION,
	CASE WHEN pl.IsTaxIncluded = 'Y'
		THEN il.PriceActual / 1.1 * il.QtyInvoiced
		ELSE il.LineNetAmt
	END LineNetAmt,
	parse('AD_Org',i.AD_Org_ID,9) ORG_NAME,
	il.PriceEntered,
	CASE WHEN pl.IsTaxIncluded = 'Y'
		THEN il.PriceActual / 1.1
		ELSE il.PriceActual
	END PriceActual,
	il.QtyInvoiced,
	parse('C_UOM',il.C_UOM_ID,0) UOM,
	(SELECT COALESCE(SUM(CASE WHEN pl.IsTaxIncluded = 'Y' THEN ilx.PriceActual / 1.1 * ilx.QtyInvoiced ELSE ilx.LineNetAmt END),0)
		FROM C_InvoiceLine ilx
		INNER JOIN C_Invoice ix ON (ilx.C_Invoice_ID = ix.C_Invoice_ID)
		WHERE ix.Z_TaxInvoice_ID = ti.Z_TaxInvoice_ID
		AND ilx.M_Product_ID = 1000002
	 ) SUM_DP_LineNetAmt,
	CURRENCYRATE(i.C_Currency_ID,303,i.DateInvoiced,114,i.AD_Client_ID,i.AD_Org_ID) CURRENCYRATE,
	dt.DocumentNote,
	bpu.Name BPU_NAME,
	ti.TaxBaseAmt,
	ti.TaxAmt
FROM C_InvoiceLine il
INNER JOIN C_Invoice i ON (i.C_Invoice_ID = il.C_Invoice_ID)
INNER JOIN Z_TaxInvoice ti ON (ti.Z_TaxInvoice_ID = i.Z_TaxInvoice_ID)
INNER JOIN C_BPartner bp ON (bp.C_BPartner_ID = i.C_BPartner_ID)
INNER JOIN AD_OrgInfo oi ON (oi.AD_Org_ID = i.AD_Org_ID)
LEFT JOIN M_Product p ON (p.M_Product_ID = il.M_Product_ID)
LEFT JOIN C_Charge c ON (c.C_Charge_ID = il.C_Charge_ID)
INNER JOIN C_DocType dt ON (dt.C_DocType_ID = ti.C_DocType_ID)
LEFT JOIN AD_User u ON (u.AD_User_ID = ti.AD_User_ID)
LEFT JOIN C_BPartner bpu ON (bpu.C_BPartner_ID = u.C_BPartner_ID)
LEFT JOIN C_BPartner_Location bpl ON (bpl.C_BPartner_ID = bp.C_BPartner_ID AND bpl.IsActive='Y' AND bpl.IsTaxTo='Y')
LEFT JOIN M_PriceList pl ON (pl.M_PriceList_ID = i.M_PriceList_ID)
WHERE	1 = 1
	AND ti.Z_TaxInvoice_ID = COALESCE($P{RECORD_ID},$P{Z_TaxInvoice_ID})
	AND il.M_Product_ID <> 1000002

UNION

-- DPP ONLY
SELECT
	ti.Z_TaxInvoice_ID,
	ti.DocumentNo TAXINVOICE_NO,
	ti.DateInvoiced,
	i.POReference,
	parse('C_PaymentTerm',i.C_PaymentTerm_ID,1) PAYMENT_TERM,
	bp.Name BP_NAME,
	bp.Value BP_CODE,
	parse('C_BPartner_Location',COALESCE(bpl.C_BPartner_Location_ID,i.C_BPartner_Location_ID),0) BP_LOCATION,
	bp.TaxID BP_TAXID,
	parse('C_Currency',i.C_Currency_ID,0) CURRENCY,
	parse('AD_Org',i.AD_Org_ID,0) ORG_DESC,
	parse('C_Location',oi.C_Location_ID,0) ORG_LOCATION,
	oi.Phone ORG_PHONE,
	oi.Fax ORG_FAX,
	oi.TaxID ORG_TAXID,
	COALESCE(p.Name,c.Name) PRODUCT_NAME,
	COALESCE(p.Value,c.Name) PRODUCT_CODE,
	COALESCE(p.Description,c.Description) PRODUCT_DESCRIPTION,
	CASE WHEN pl.IsTaxIncluded = 'Y'
		THEN il.PriceActual / 1.1 * il.QtyInvoiced
		ELSE il.LineNetAmt
	END LineNetAmt,
	parse('AD_Org',i.AD_Org_ID,9) ORG_NAME,
	il.PriceEntered,
	CASE WHEN pl.IsTaxIncluded = 'Y'
		THEN il.PriceActual / 1.1
		ELSE il.PriceActual
	END PriceActual,
	il.QtyInvoiced,
	parse('C_UOM',il.C_UOM_ID,0) UOM,
	0 SUM_DP_LineNetAmt,
	CURRENCYRATE(i.C_Currency_ID,303,i.DateInvoiced,114,i.AD_Client_ID,i.AD_Org_ID) CURRENCYRATE,
	dt.DocumentNote,
	bpu.Name BPU_NAME,
	ti.TaxBaseAmt,
	ti.TaxAmt
FROM C_InvoiceLine il
INNER JOIN C_Invoice i ON (i.C_Invoice_ID = il.C_Invoice_ID)
INNER JOIN Z_TaxInvoice ti ON (ti.Z_TaxInvoice_ID = i.Z_TaxInvoice_ID)
INNER JOIN C_BPartner bp ON (bp.C_BPartner_ID = i.C_BPartner_ID)
INNER JOIN AD_OrgInfo oi ON (oi.AD_Org_ID = i.AD_Org_ID)
LEFT JOIN M_Product p ON (p.M_Product_ID = il.M_Product_ID)
LEFT JOIN C_Charge c ON (c.C_Charge_ID = il.C_Charge_ID)
INNER JOIN C_DocType dt ON (dt.C_DocType_ID = ti.C_DocType_ID)
LEFT JOIN AD_User u ON (u.AD_User_ID = ti.AD_User_ID)
LEFT JOIN C_BPartner bpu ON (bpu.C_BPartner_ID = u.C_BPartner_ID)
LEFT JOIN C_BPartner_Location bpl ON (bpl.C_BPartner_ID = bp.C_BPartner_ID AND bpl.IsActive='Y' AND bpl.IsTaxTo='Y')
LEFT JOIN M_PriceList pl ON (pl.M_PriceList_ID = i.M_PriceList_ID)
WHERE	1 = 1
	AND ti.Z_TaxInvoice_ID = COALESCE($P{RECORD_ID},$P{Z_TaxInvoice_ID})
	AND NOT EXISTS(	SELECT 1 FROM C_InvoiceLine ilx
			INNER JOIN C_Invoice ix ON (ilx.C_Invoice_ID = ix.C_Invoice_ID)
			WHERE ix.Z_TaxInvoice_ID = ti.Z_TaxInvoice_ID AND ilx.M_Product_ID <> 1000002
			)
) z
LEFT JOIN
(
SELECT
        ix.Z_TaxInvoice_ID,
        ROW_NUMBER() OVER (PARTITION BY iox.MovementDate ORDER BY iox.MovementDate DESC) AS io_id,
        iox.DocumentNo SHIPMENT_NO,
        iox.MovementDate,
        parse('C_BPartner',iox.C_BPartner_ID,1) SHIPMENT_BP,
        parse('C_BPartner_Location',iox.C_BPartner_Location_ID,0) SHIPMENT_LOCATION
FROM M_InOut iox
INNER JOIN M_InOutLine iolx ON (iolx.M_InOut_ID = iox.M_InOut_ID)
INNER JOIN C_InvoiceLine ilx ON (ilx.M_InOutLine_ID = iolx.M_InOutLine_ID)
INNER JOIN C_Invoice ix ON (ix.C_Invoice_ID = ilx.C_Invoice_ID)
) x ON (x.Z_TaxInvoice_ID = z.Z_TaxInvoice_ID)
ORDER BY z.PRODUCT_CODE,z.PriceEntered]]>
	</queryString>
